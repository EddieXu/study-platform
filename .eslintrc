{
  "extends": "react-app",
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": 2,
    "import/first": "off",
    "react-hooks/exhaustive-deps": "warn",
    "prettier/prettier": "off",
    "no-undef": "off",
    "jsx-a11y/alt-text": 0
  },
  "plugins": [
    "prettier"
  ]
}